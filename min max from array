#include <bits/stdc++.h>
using namespace std;

 
void minMax(int arr[], int n)
{
    int min,max;
    if(arr[0]>arr[1])
    {
        min=arr[1];
        max=arr[0];
    }
    else{
      min=arr[0];
      max=arr[1];
           
    }
    for(int i=2;i<n;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
        if(arr[i]<min)
        {
            min=arr[i];
        }
    }
    cout<<"min  "<<min<<endl<<"max  "<<max;
}
int main() {
	// your code goes here
	int arr[]={10,30,11,12,9};
	int n = sizeof(arr)/sizeof(int);
	minMax(arr, n);


	return 0;
}
