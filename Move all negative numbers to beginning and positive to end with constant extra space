#include <bits/stdc++.h>
using namespace std;

void swap (int *a, int *b)
{
    int temp ;
    temp = *a;
    *a = *b;
    *b = temp;
}
int moveNumbers(int arr[],int n)
{
    int low=0 , mid =0, high = n-1 ;
    while(mid<high){
        if(arr[mid]<0)
        {
          swap(arr[mid], arr[low]);
          mid++;
          low++;
        }
        if(arr[mid]>=0)
        {
            swap(arr[mid], arr[high]);
            high -- ;
        }
    }
}
int main() {
	// your code goes here
	int arr[]={-10,-30,11,12,-9};
	int n = sizeof(arr)/sizeof(int);
	moveNumbers(arr, n);
	for(int i=0 ; i<n ;i++)
	{
	    cout<<arr[i]<<" ";
	}


	return 0;
}
